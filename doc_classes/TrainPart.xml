<?xml version="1.0" encoding="UTF-8" ?>
<class name="TrainPart" inherits="Node" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="https://raw.githubusercontent.com/godotengine/godot/master/doc/class.xsd">
	<brief_description>
		Basic train part class. Handles all engines etc.
	</brief_description>
	<description>
		Basic train part class
	</description>
	<tutorials>
	</tutorials>
	<methods>
		<method name="bind_command" is_experimental="true">
			<return type="void" />
			<param index="0" name="command" type="String" />
			<param index="1" name="callable" type="Callable" />
			<description>
			</description>
		</method>
		<method name="broadcast_command">
			<return type="void" />
			<param index="0" name="command" type="String" />
			<param index="1" name="p1" type="Variant" default="null" />
			<param index="2" name="p2" type="Variant" default="null" />
			<description>
				Broadcast command to all trains (including the train, which part belongs to)
			</description>
		</method>
		<method name="emit_config_changed_signal">
			<return type="void" />
			<description>
				Emits the "config_changed" signal
			</description>
		</method>
		<method name="get_mover_state">
			<return type="Dictionary" />
			<description>
				Returns dictionary ov properties with values from iternal mover
			</description>
		</method>
		<method name="log">
			<return type="void" />
			<param index="0" name="loglevel" type="int" enum="TrainSystem.TrainLogLevel" />
			<param index="1" name="line" type="String" />
			<description>
				Logs a message
			</description>
		</method>
		<method name="log_debug">
			<return type="void" />
			<param index="0" name="line" type="String" />
			<description>
				Logs a debug message. See [TrainLogLevel].
			</description>
		</method>
		<method name="log_error">
			<return type="void" />
			<param index="0" name="line" type="String" />
			<description>
				Logs an error message. See [TrainLogLevel].
			</description>
		</method>
		<method name="log_info">
			<return type="void" />
			<param index="0" name="line" type="String" />
			<description>
				Logs an info message. See [TrainLogLevel].
			</description>
		</method>
		<method name="log_warning">
			<return type="void" />
			<param index="0" name="line" type="String" />
				Logs a warning message. See [TrainLogLevel].
			<description>
			</description>
		</method>
		<method name="on_command_received">
			<return type="void" />
			<param index="0" name="_unnamed_arg0" type="String" />
			<param index="1" name="_unnamed_arg1" type="Variant" />
			<param index="2" name="_unnamed_arg2" type="Variant" />
			<description>
				Handles a command received by [TrainController]
			</description>
		</method>
		<method name="send_command">
			<return type="void" />
			<param index="0" name="command" type="String" />
			<param index="1" name="p1" type="Variant" default="null" />
			<param index="2" name="p2" type="Variant" default="null" />
			<description>
				Sends command to the train, which part belongs to.
			</description>
		</method>
		<method name="unbind_command" is_experimental="true">
			<return type="void" />
			<param index="0" name="command" type="String" />
			<param index="1" name="callable" type="Callable" />
			<description>
			</description>
		</method>
		<method name="update_mover">
			<return type="void" />
			<description>
				Performs update on an iternal mover of [TrainController]
			</description>
		</method>
	</methods>
	<members>
		<member name="enabled" type="bool" setter="set_enabled" getter="get_enabled" default="true">
			If true, this train part will be processed by [TrainController] during updating and fetching a mover's state.
		</member>
	</members>
	<signals>
		<signal name="config_changed">
			<description>
				Emitted when the internal config has changed
			</description>
		</signal>
		<signal name="enable_changed">
			<param index="0" name="enabled" type="bool" />
			<description>
				Train part status was enabled or disabled
			</description>
		</signal>
		<signal name="train_part_disabled">
			<description>
				Train part becomes disabled
			</description>
		</signal>
		<signal name="train_part_enabled">
			<description>
				Train part becomes enabled
			</description>
		</signal>
	</signals>
</class>
